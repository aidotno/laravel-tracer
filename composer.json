{
    "name": "protonemedia/laravel-tracer",
    "description": "",
    "keywords": [
        "protonemedia",
        "laravel-tracer"
    ],
    "homepage": "https://github.com/pascalbaljetmedia/laravel-tracer",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Pascal Baljet",
            "email": "pascal@protone.media",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "5.7.*|5.8.*|^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*|3.9.*",
        "phpunit/phpunit": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Protonemedia\\LaravelTracer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Protonemedia\\LaravelTracer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Protonemedia\\LaravelTracer\\LaravelTracerServiceProvider"
            ]
        }
    }
}
